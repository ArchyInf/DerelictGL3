GLVersion loadContext( alias ctx)( GLVersion glversion ) {
    auto ret = loadCore!ctx( glversion );
    loadARB!ctx( glversion );

    static if( ctx.EXTEnabled ) loadEXT!ctx( glversion );
    return ret;
}

